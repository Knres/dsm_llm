<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ApplicationCore" namespace="ApplicationCore.Domain.EN">
  <class name="Reporte" table="Reporte">
    <id name="Id" column="Id" type="long">
      <generator class="hilo">
        <param name="table">NHibernateUniqueKey</param>
        <param name="column">NextHigh</param>
        <param name="max_lo">100</param>
      </generator>
    </id>
    <property name="Motivo" column="Motivo" />
    <property name="Estado" column="Estado" />
    <property name="Fecha" column="Fecha" />

    <many-to-one name="Usuario" column="Usuario_Id" class="Usuario" not-null="true" />
    <many-to-one name="Administrador" column="Administrador_Id" class="Administrador" not-null="false" />
    <many-to-one name="Resena" column="Resena_Id" class="Resena" not-null="false" />
  </class>
</hibernate-mapping>
