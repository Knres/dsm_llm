<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="ApplicationCore.Domain.EN.Lista, ApplicationCore" table="Lista">
    <id name="Id" column="Id" type="long">
      <generator class="hilo">
        <param name="table">NHibernateUniqueKey</param>
        <param name="column">NextHigh</param>
        <param name="max_lo">100</param>
      </generator>
    </id>
    <property name="Nombre" />
    <property name="Tipo" />

    <many-to-one name="Creador" class="ApplicationCore.Domain.EN.Usuario, ApplicationCore" column="CreadorId" />

    <set name="Peliculas" table="ListaPelicula" cascade="none" inverse="false" lazy="true">
      <key column="ListaId" />
      <many-to-many class="ApplicationCore.Domain.EN.Pelicula, ApplicationCore" column="PeliculaId" />
    </set>
  </class>
</hibernate-mapping>
