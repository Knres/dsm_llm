<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="ApplicationCore.Domain.EN.Resenya, ApplicationCore" table="Resenya">
    <id name="Id" column="Id" type="long">
      <generator class="hilo">
        <param name="table">NHibernateUniqueKey</param>
        <param name="column">NextHigh</param>
        <param name="max_lo">100</param>
      </generator>
    </id>
    <property name="Punctuation" type="long" />
    <property name="Comentario" type="string" />
    <property name="Fecha" type="DateTime" />

    <many-to-one name="Autor" class="ApplicationCore.Domain.EN.Usuario, ApplicationCore" column="AutorId" />
    <many-to-one name="Pelicula" class="ApplicationCore.Domain.EN.Pelicula, ApplicationCore" column="PeliculaId" />
  </class>
</hibernate-mapping>