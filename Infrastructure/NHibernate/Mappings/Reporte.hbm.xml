<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="ApplicationCore.Domain.EN.Reporte, ApplicationCore" table="Reporte">
    <id name="Id" column="Id" type="long">
      <generator class="hilo">
        <param name="table">NHibernateUniqueKey</param>
        <param name="column">NextHigh</param>
        <param name="max_lo">100</param>
      </generator>
    </id>
    <property name="Motivo" />
    <property name="Estado" />
    <property name="Fecha" />

    <many-to-one name="Autor" class="ApplicationCore.Domain.EN.Usuario, ApplicationCore" column="AutorId" />
    <many-to-one name="SobreResena" class="ApplicationCore.Domain.EN.Resena, ApplicationCore" column="ResenaId" />
  </class>
</hibernate-mapping>
