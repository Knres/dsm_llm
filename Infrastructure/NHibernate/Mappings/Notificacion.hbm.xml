<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="ApplicationCore.Domain.EN.Notificacion, ApplicationCore" table="Notificacion">
    <id name="Id" column="Id" type="long">
      <generator class="hilo">
        <param name="table">NHibernateUniqueKey</param>
        <param name="column">NextHigh</param>
        <param name="max_lo">100</param>
      </generator>
    </id>
    <property name="Mensaje" />
    <property name="Fecha" />
    <property name="Tipo" />
    <property name="IdOrigen" />
  <property name="Leida" />
  <property name="FechaLeida" />
  <property name="EstadoNotificacion" type="ApplicationCore.Domain.Enums.estadoNotificacion, ApplicationCore" />

    <many-to-one name="Destinatario" class="ApplicationCore.Domain.EN.Usuario, ApplicationCore" column="DestinatarioId" />
  </class>
</hibernate-mapping>
