<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="ApplicationCore" namespace="ApplicationCore.Domain.EN">
  <class name="Notificacion" table="Notificacion">
    <id name="Id" column="Id" type="long">
      <generator class="hilo">
        <param name="table">NHibernateUniqueKey</param>
        <param name="column">NextHigh</param>
        <param name="max_lo">100</param>
      </generator>
    </id>
    <property name="Mensaje" column="Mensaje" />
    <property name="Fecha" column="Fecha" />
    <property name="Tipo" column="Tipo" />
    <property name="IdOrigen" column="IdOrigen" />

    <many-to-one name="Usuario" column="Usuario_Id" class="Usuario" not-null="true" />
  </class>
</hibernate-mapping>
