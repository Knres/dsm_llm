<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="ApplicationCore.Domain.EN.Resena, ApplicationCore" table="Resena">
    <id name="Id" column="Id" type="long">
      <generator class="hilo">
        <param name="table">NHibernateUniqueKey</param>
        <param name="column">NextHigh</param>
        <param name="max_lo">100</param>
      </generator>
    </id>
    <property name="Puntuacion" />
    <property name="Comentario" />
    <property name="FechaPublicacion" />

    <many-to-one name="Autor" class="ApplicationCore.Domain.EN.Usuario, ApplicationCore" column="AutorId" />
    <many-to-one name="Pelicula" class="ApplicationCore.Domain.EN.Pelicula, ApplicationCore" column="PeliculaId" />
  </class>
</hibernate-mapping>
